    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "dappy-cli": "0.4.2",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "showdown": "^1.9.1"
