{
  "name": "dapp-page",
  "version": "1.0.2",
  "description": "Personal page on Dappy",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "dappy:init": "node ./node_modules/dappy-cli/lib/ init",
